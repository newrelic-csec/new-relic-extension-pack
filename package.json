{
  "name": "new-relic-extension-pack",
  "displayName": "New Relic Extension Pack",
  "description": "A collection of VS Code Extensions for New Relic programmability.",
  "keywords": [
    "New Relic",
    "NRQL",
    "Nerdpack",
    "Synthetics",
    "Programmability"
  ],
  "icon": "assets/nr-logo.png",
  "galleryBanner": {
    "color": "#0AB0BF",
    "theme": "light"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "version": "0.0.1",
  "publisher": "new-relic",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "extensionPack": [
    "new-relic.nr1",
    "joelalejandro.nrql-language",
    "BenedictoTan.step-line-generator"
